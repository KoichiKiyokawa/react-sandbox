{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "rimraf build public/build && pnpm run build:css && remix build",
    "build:css": "postcss 'app/styles/*.postcss' -o ./app/styles/generated.css",
    "dev": "concurrently \"pnpm run dev:css\" \"remix dev\"",
    "dev:css": "pnpm run build:css -- --watch",
    "postinstall": "prisma generate",
    "start": "remix-serve build",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "fmt": "prettier --write .",
    "fix:all": "pnpm run lint --fix && pnpm run fmt && prisma format && tsc --noEmit",
    "check:all": "pnpm run lint && pnpm run fmt --check && tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "2.9.10",
    "@prisma/client": "4.16.2",
    "@remix-run/node": "1.12.0",
    "@remix-run/react": "1.12.0",
    "@remix-run/serve": "1.12.0",
    "bcryptjs": "2.4.3",
    "classnames": "2.3.2",
    "dayjs": "1.11.7",
    "http-status-codes": "2.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.0",
    "react-icons": "4.7.1",
    "zod": "3.20.2"
  },
  "devDependencies": {
    "@remix-run/dev": "1.12.0",
    "@remix-run/eslint-config": "1.12.0",
    "@types/bcryptjs": "2.4.2",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "10.4.13",
    "concurrently": "7.6.0",
    "esbuild-register": "3.4.2",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-cli": "10.1.0",
    "prettier": "2.8.3",
    "prisma": "4.16.2",
    "rimraf": "3.0.2",
    "tailwindcss": "3.2.4",
    "type-fest": "3.5.4",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "packageManager": "pnpm@7.26.3"
}
