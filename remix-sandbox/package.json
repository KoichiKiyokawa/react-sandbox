{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run windi:build && remix build",
    "dev": "concurrently \"npm run windi:base -- --dev\" \"remix dev\"",
    "windi:build": "npm run windi:base -- --minify",
    "windi:base": "windicss 'app/**/*.tsx' --output app/styles/generated.css --preflight",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "lint:fix": "eslint . --fix --ext .ts,.tsx && prettier --write . --ignore-path .gitignore"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.8.3",
    "@prisma/client": "^3.5.0",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "classnames": "^2.3.1",
    "dayjs": "^1.10.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.20.4",
    "remix": "^1.0.6",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^6.4.0",
    "esbuild-register": "^3.2.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.0",
    "prisma": "^3.5.0",
    "type-fest": "^2.6.0",
    "typescript": "^4.1.2",
    "windicss": "^3.2.1"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  }
}
