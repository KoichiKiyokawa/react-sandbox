{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "rimraf build public/build && pnpm run build:css && remix build",
    "build:css": "postcss 'app/styles/*.postcss' -o ./app/styles/generated.css",
    "dev": "concurrently \"pnpm run dev:css\" \"remix dev\"",
    "dev:css": "pnpm run build:css -- --watch",
    "postinstall": "remix setup node && prisma generate",
    "start": "remix-serve build",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "fmt": "prettier --write .",
    "fix:all": "pnpm run lint --fix && pnpm run fmt && prisma format && tsc --noEmit",
    "check:all": "pnpm run lint && pnpm run fmt -- --check && tsc --noEmit"
  },
  "dependencies": {
    "@hookform/resolvers": "2.8.8",
    "@prisma/client": "3.12.0",
    "@remix-run/react": "1.3.4",
    "@remix-run/serve": "1.3.2",
    "bcryptjs": "2.4.3",
    "classnames": "2.3.1",
    "dayjs": "1.11.0",
    "http-status-codes": "2.2.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "7.29.0",
    "react-icons": "4.3.1",
    "remix": "1.3.4",
    "zod": "3.14.4"
  },
  "devDependencies": {
    "@remix-run/dev": "1.3.2",
    "@remix-run/eslint-config": "1.3.5",
    "@types/bcryptjs": "2.4.2",
    "@types/react": "18.0.0",
    "@types/react-dom": "18.0.0",
    "autoprefixer": "10.4.4",
    "concurrently": "7.1.0",
    "esbuild-register": "3.3.2",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.12",
    "postcss-cli": "9.1.0",
    "prettier": "2.6.2",
    "prisma": "3.12.0",
    "rimraf": "3.0.2",
    "tailwindcss": "3.0.23",
    "type-fest": "2.12.2",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "packageManager": "pnpm@6.32.4"
}
