{
  "name": "cloudflare-next-on-pages-d1",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:pages": "wrangler pages dev --d1=DB --compatibility-flag=nodejs_compat -- next dev",
