    "@types/better-sqlite3": "^7.6.4",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "autoprefixer": "10.4.20",
    "better-sqlite3": "8.7.0",
    "concurrently": "8.2.2",
    "drizzle-kit": "0.30.5",
    "eslint": "8.57.1",
    "eslint-config-next": "13.5.8",
    "postcss": "8.5.3",
    "tailwindcss": "3.4.17",
