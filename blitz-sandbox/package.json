{
  "name": "blitz-sandbox",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "3.12.0",
    "blitz": "0.45.4",
    "react-dom": "18.1.0",
    "react": "18.1.0",
    "zod": "3.14.4",
    "react-hook-form": "7.30.0",
    "@hookform/resolvers": "2.8.8"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "18.0.6",
    "eslint": "8.14.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.0",
    "prettier-plugin-prisma": "3.13.0",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5",
    "prisma": "3.12.0",
    "typescript": "4.6.3"
  },
  "private": true
}
