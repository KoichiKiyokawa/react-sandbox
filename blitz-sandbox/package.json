{
  "name": "blitz-sandbox",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "4.5.0",
    "blitz": "0.45.5",
    "react-dom": "18.3.1",
    "react": "18.3.1",
    "zod": "3.25.76",
    "react-hook-form": "7.71.1",
    "@hookform/resolvers": "2.9.11"
  },
  "devDependencies": {
    "@types/preview-email": "2.0.1",
    "@types/react": "18.3.28",
    "eslint": "8.57.1",
    "husky": "8.0.3",
    "lint-staged": "13.3.0",
    "prettier-plugin-prisma": "4.17.0",
    "prettier": "2.8.8",
    "pretty-quick": "3.3.1",
    "preview-email": "3.1.1",
    "prisma": "4.5.0",
    "typescript": "4.9.5"
  },
  "private": true
}
